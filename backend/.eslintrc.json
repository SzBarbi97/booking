{
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "eslint-config-prettier"
  ],
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "quotes": [
      "error",
      "single"
    ]
  }
}